<Window x:Class="latihan.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:latihan"
        mc:Ignorable="d"
        Title="MainWindow" Height="321" Width="519">
    <Grid Margin="0,0,0,7">
        <Button x:Name="btn_add" Content="Tambahkan" HorizontalAlignment="Left" Margin="47,221,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.83,-1.74" Width="79" Click="Button_Click"/>
        <TextBox x:Name="txt_nama" HorizontalAlignment="Left" Margin="47,66,0,0" TextWrapping="Wrap" Text="Nama barang" VerticalAlignment="Top" Width="153"/>
        <TextBox x:Name="txt_jenis" HorizontalAlignment="Left" Margin="47,113,0,0" TextWrapping="Wrap" Text="Jenis Barang" VerticalAlignment="Top" Width="153"/>
        <TextBox x:Name="txt_harga" HorizontalAlignment="Left" Margin="47,162,0,0" TextWrapping="Wrap" Text="Harga Barang" VerticalAlignment="Top" Width="153"/>
        <ListBox x:Name="daftar_barang" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="286,66,0,0"/>

    </Grid>
</Window>
